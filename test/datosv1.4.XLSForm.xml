      <bind nodeset="/datosv1.4.XLSForm/data/plantData/chlorineDose" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/entranceWaterLevel1" relevant=" /datosv1.4.XLSForm/data/nFilters  &gt; 0" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/entranceWaterLevel2" relevant=" /datosv1.4.XLSForm/data/nFilters  &gt; 1" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/entranceWaterLevel3" relevant=" /datosv1.4.XLSForm/data/nFilters  &gt; 2" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/entranceWaterLevel4" relevant=" /datosv1.4.XLSForm/data/nFilters  &gt; 3" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/entranceWaterLevel5" relevant=" /datosv1.4.XLSForm/data/nFilters  &gt; 4" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/entranceWaterLevel6" relevant=" /datosv1.4.XLSForm/data/nFilters  &gt; 5" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/backwashed" relevant=" /datosv1.4.XLSForm/data/nFilters  &gt; 0" type="select1"/>
      <bind nodeset="/datosv1.4.XLSForm/data/backwash" relevant="selected(/datosv1.4.XLSForm/data/purpose, 'backwash') or  /datosv1.4.XLSForm/data/plantData/backwashed  = 'yes'"/>
