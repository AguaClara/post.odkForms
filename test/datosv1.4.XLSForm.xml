      <bind nodeset="/datosv1.4.XLSForm/data/plantData/flowRate" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/coagulantDose" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/coagulantCheck" relevant=" /datosv1.4.XLSForm/data/plantData/coagulantDose  &gt;= 50" type="string"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/CDCPos" required="true()" type="int"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/rawWaterTurbidity" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/settledWaterTurbidity" required="true()" type="decimal"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/setlledWaterTurbidityCheck" relevant=" /datosv1.4.XLSForm/data/plantData/settledWaterTurbidity  &gt;= 20" type="string"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData/filteredWaterTurbidity" required="true()" type="decimal"/>
