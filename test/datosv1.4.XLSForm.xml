      <bind nodeset="/datosv1.4.XLSForm/data/plant" required="true()" type="select1"/>
      <bind calculate="pulldata('nFilters', 'n', 'plantName_key',  /datosv1.4.XLSForm/data/plant )" nodeset="/datosv1.4.XLSForm/data/nFilters" type="string"/>
      <bind nodeset="/datosv1.4.XLSForm/data/purpose" type="select"/>
      <bind nodeset="/datosv1.4.XLSForm/data/plantData" relevant="selected( /datosv1.4.XLSForm/data/purpose , 'plantData')"/>
